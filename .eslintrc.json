{
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }],
    "max-depth": ["error", { "max": 1 }],
    "max-lines-per-function": ["error", { "max": 10 }]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"]
}
